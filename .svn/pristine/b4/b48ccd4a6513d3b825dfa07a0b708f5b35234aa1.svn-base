package com.dls.base.entity;

import javax.persistence.*;

@Entity
public class Foo {
 
    @Id
	@GeneratedValue(strategy = GenerationType.AUTO)
    @Column(name = "ID")
    private long id;
  
    @Column(name = "NAME")
    private String name;
 
    public long getId(){
    	return id;
	}

	public void setId(long id){
    	this.id = id;
	}

	public String getName(){
		return name;
	}

	public void setName(String name){
		this.name = name;
	}

}